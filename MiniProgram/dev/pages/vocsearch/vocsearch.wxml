<search 
  value="{{ value }}"
  placeholder="Entrez votre mot à rechercher"
  use-action-slot
  bind:search="onSearch"
  bind:change="onChange"
  shape="round"
  background="#1296db"
  left-icon=""
  input-align="center">
    <view class="iconfont icon-sousuo" slot="action" bind:tap="onClick">
      搜索
    </view>
</search>

<view
  class="result_list"
  >
     <navigator
       class="redsult_item"
       target=""
       url=""
       hover-class="navigator-hover"
       open-type="navigate"
       wx:for="{{ resultList }}"
       wx:id="result_id">
       
     </navigator>
</view>